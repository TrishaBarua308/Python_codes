a=[1,2,3,4,5]
n=len(a)
i=0
result=0

while i<n:
    result = result + a[i]
    i= i+1 

print("Result ",result)


b = [2,-4,3, -45, 29 , -54,-45]

i=0
while i<len(b):
    if b[i]<0:
        b[i]=0
    i+=1 

print(b)
        
        
        
        
        
        
        
        
        
        
        
        
        
